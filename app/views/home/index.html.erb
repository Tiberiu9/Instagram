<div class="d-flex pt-5 mx-auto">
  <div class="d-flex flex-column border border-1 px-0" style=" max-width: 634px; ">
    <%= render 'story/stories_list' %>
    <div class="mx-auto">
      <% current_user.posts.each do |p| %>
        <%= render 'posts/post', post: p %>
      <% end %>
    </div>
  </div>
  <div class="d-none d-xl-block">
    <%= render 'suggestions/suggestions_list'%>
  </div>
</div>
